{
  "schemaVersion": 1,
  "id": "quickarmorswap",
  "version": "${version}",

  "name": "Quick Armor Swap",
  "description": "Just a simple small mod to swap an equipped armor piece with the one in your hand",
  "authors": [
    "SuperPro148"
  ],
  "contact": {
    "homepage": "https://SuperPro148.github.io/pages/mcmods.html",
    "sources": "https://github.com/SuperPro148/quickarmorswap/",
    "issues": "https://github.com/SuperPro148/quickarmorswap/issues"
  },

  "license": "GPL-3.0-only",
  "icon": "assets/quickarmorswap/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.superpro148.quickarmorswap.QuickArmorSwap"
    ],
    "modmenu": [
      "io.github.superpro148.quickarmorswap.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "quickarmorswap.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "~1.19",
    "java": ">=17",
    "yet-another-config-lib": "*",
    "modmenu": "*",
    "configlib148": "*"
  },
  "suggests": {
  }
}
